            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="hjc.iraq.com.omdbcodelap.activities.ScondActivity">
        </activity>
        <activity android:name="hjc.iraq.com.omdbcodelap.activities.SearchActivity">
        </activity>
        <activity android:name="hjc.iraq.com.omdbcodelap.activities.ConstraintActivity">
        </activity>
        <activity android:name="hjc.iraq.com.omdbcodelap.activities.GridActivity">
        </activity>
    </application>

</manifest>